{
    "name": "abolfazl-talebi/laravel-tutorial",
    "type": "project",
    "description": "The Laravel Framework Tutorial Sources on Alefyar.com.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "homepage": "https://github.com/Abolfazl-Talebi/laravel-tutorial",
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
  "repositories": [
    {
