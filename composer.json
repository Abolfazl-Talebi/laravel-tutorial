       "url": "https://github.com/Abolfazl-Talebi/laravel-tutorial" 
    }
  ],
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload-dev": {
